{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "^db:generate"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^db:push"]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false,
      "dependsOn": ["db:generate"]
    }
  }
}
